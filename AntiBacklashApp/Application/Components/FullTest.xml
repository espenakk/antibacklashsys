<Component Model="AntiBacklashLib.FullTest" Name="FullTest" src="Components/FullTest.xml" Description="Automatic full test with different parameters">
  <SchedulingOrder>TopDown</SchedulingOrder>
  <SchedulingGroup>Default</SchedulingGroup>
  <fs>100</fs>
  <Status>0</Status>

  <Signals>
    <Signal Description="Process run time each s." Input="0" Model="CDPSignal&lt;double&gt;" Name="Process Timer" Type="double" Unit="s/s"></Signal>
    <Signal Description="Process interval [s]." Input="0" Model="CDPSignal&lt;double&gt;" Name="Process Period" Type="double" Unit="s"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;int&gt;" Name="AntiBacklashMode" Type="int"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="LoadTorqueLimit" Type="double" Unit="Nm" Value="4"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="MaxTorque" Type="double" Unit="Nm" Value="5"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="SlaveTorqueBase" Type="double" Unit="Nm" Value="3"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="SlaveTorqueGain" Type="double" Unit="Nm" Value="5"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="Offset" Type="double" Unit="%" Value="1"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="SlaveDroop" Type="double" Unit="%"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="MasterDroop" Type="double" Unit="%"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;bool&gt;" Name="Start" Type="bool"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="MaxSpeed" Type="double" Unit="rad/s" Value="1.25"></Signal>
    <Signal Input="1" Model="CDPSignal&lt;bool&gt;" Name="Enable" Type="bool"></Signal>
    <Signal Input="1" Model="CDPSignal&lt;bool&gt;" Name="IsRunning" Type="bool" Routing="..AntiBacklashController.Running"></Signal>
  </Signals>

  <Alarms>
    <Alarm Description="A component was suspended!" Name="Component Suspended" Text="Component was suspended!"></Alarm>
    <Alarm Description="Alarm is triggered (with Text containing the list of errors) when some node of the component (like operator) has configuration fault." Level="Error" Name="ConfigurationFaults"></Alarm>
  </Alarms>

  <Parameters>
    <Parameter Name="NumTests" Value="4"></Parameter>
  </Parameters>
</Component>