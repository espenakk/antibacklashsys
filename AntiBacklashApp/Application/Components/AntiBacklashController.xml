<Component Model="AntiBacklashLib.AntiBacklashController" Name="AntiBacklashController" src="Components/AntiBacklashController.xml" Description="Controller for anti backlash testing">
  <SchedulingOrder>TopDown</SchedulingOrder>
  <SchedulingGroup>Default</SchedulingGroup>
  <fs>100</fs>
  <Status>0</Status>

  <Signals>
    <Signal Description="Process run time each s." Input="0" Model="CDPSignal&lt;double&gt;" Name="Process Timer" Type="double" Unit="s/s"></Signal>
    <Signal Description="Process interval [s]." Input="0" Model="CDPSignal&lt;double&gt;" Name="Process Period" Type="double" Unit="s"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="SpeedRef" Type="double" Unit="rad/s"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="ENC1Speed" Type="double" Unit="rad/s"></Signal>
    <Signal Input="1" Model="CDPSignal&lt;int&gt;" Name="AntiBacklashMode" Type="int" Routing="..FullTest.AntiBacklashMode"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="FC1Speed" Type="double" Unit="rad/s"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="FC2Speed" Type="double" Unit="rad/s"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="FC1Torque" Type="double" Unit="Nm"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="FC2Torque" Type="double" Unit="Nm"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="FC3Torque" Type="double" Unit="Nm"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="ENC1Position" Type="double" Unit="째"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;bool&gt;" Name="Running" Type="bool"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;int&gt;" Name="TestIndex" Type="int"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="FC1Position" Type="double" Unit="째"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="FC2Position" Type="double" Unit="째"></Signal>
    <Signal Input="0" Model="CDPSignal&lt;double&gt;" Name="FC3Position" Type="double" Unit="째"></Signal>
  </Signals>

  <Alarms>
    <Alarm Description="A component was suspended!" Name="Component Suspended" Text="Component was suspended!"></Alarm>
    <Alarm Description="Alarm is triggered (with Text containing the list of errors) when some node of the component (like operator) has configuration fault." Level="Error" Name="ConfigurationFaults"></Alarm>
  </Alarms>

  <Ports>
    <Port Input="0" Model="VaconLib.VaconMarineAppFCPort" Name="FC1" LoadDrooping="0">

      <Connections>
        <Connection Input="0" LocalName=".Enable" Model="CDPPortConnection" Name="MapEnable"></Connection>
        <Connection Input="0" LocalName=".ModeSelect" Model="CDPPortConnection" Name="MapModeSelect"></Connection>
        <Connection Input="0" LocalName=".SpeedRef" Model="CDPPortConnection" Name="MapSpeedRef"></Connection>
        <Connection Input="0" LocalName=".TorqueRef" Model="CDPPortConnection" Name="MapTorqueRef"></Connection>
        <Connection Input="0" LocalName=".TorqueLimitMotoring" Model="CDPPortConnection" Name="MapTorqueLimitMotoring"></Connection>
        <Connection Input="0" LocalName=".TorqueLimitGeneratoring" Model="CDPPortConnection" Name="MapTorqueLimitGeneratoring"></Connection>
        <Connection Input="0" LocalName=".PowerLimitMotoring" Model="CDPPortConnection" Name="MapPowerLimitMotoring"></Connection>
        <Connection Input="0" LocalName=".PowerLimitGeneratoring" Model="CDPPortConnection" Name="MapPowerLimitGeneratoring"></Connection>
        <Connection Input="0" LocalName=".LoadDrooping" Model="CDPPortConnection" Name="MapLoadDrooping"></Connection>
        <Connection Input="0" LocalName=".MasterFollowerMode" Model="CDPPortConnection" Name="MapMasterFollowerMode"></Connection>
        <Connection Input="1" LocalName=".StatusFlags" Model="CDPPortConnection" Name="MapStatusFlags"></Connection>
        <Connection Input="1" LocalName=".SpeedActual" Model="CDPPortConnection" Name="MapSpeedActual"></Connection>
        <Connection Input="1" LocalName=".TorqueActual" Model="CDPPortConnection" Name="MapTorqueActual"></Connection>
      </Connections></Port>
    <Port Input="0" Model="VaconLib.VaconMarineAppFCPort" Name="FC2" LoadDrooping="0">

      <Connections>
        <Connection Input="0" LocalName=".Enable" Model="CDPPortConnection" Name="MapEnable"></Connection>
        <Connection Input="0" LocalName=".ModeSelect" Model="CDPPortConnection" Name="MapModeSelect"></Connection>
        <Connection Input="0" LocalName=".SpeedRef" Model="CDPPortConnection" Name="MapSpeedRef"></Connection>
        <Connection Input="0" LocalName=".TorqueRef" Model="CDPPortConnection" Name="MapTorqueRef"></Connection>
        <Connection Input="0" LocalName=".TorqueLimitMotoring" Model="CDPPortConnection" Name="MapTorqueLimitMotoring"></Connection>
        <Connection Input="0" LocalName=".TorqueLimitGeneratoring" Model="CDPPortConnection" Name="MapTorqueLimitGeneratoring"></Connection>
        <Connection Input="0" LocalName=".PowerLimitMotoring" Model="CDPPortConnection" Name="MapPowerLimitMotoring"></Connection>
        <Connection Input="0" LocalName=".PowerLimitGeneratoring" Model="CDPPortConnection" Name="MapPowerLimitGeneratoring"></Connection>
        <Connection Input="0" LocalName=".LoadDrooping" Model="CDPPortConnection" Name="MapLoadDrooping"></Connection>
        <Connection Input="0" LocalName=".MasterFollowerMode" Model="CDPPortConnection" Name="MapMasterFollowerMode"></Connection>
        <Connection Input="1" LocalName=".StatusFlags" Model="CDPPortConnection" Name="MapStatusFlags"></Connection>
        <Connection Input="1" LocalName=".SpeedActual" Model="CDPPortConnection" Name="MapSpeedActual"></Connection>
        <Connection Input="1" LocalName=".TorqueActual" Model="CDPPortConnection" Name="MapTorqueActual"></Connection>
      </Connections></Port>
    <Port Input="0" Model="VaconLib.VaconMarineAppFCPort" Name="FC3">

      <Connections>
        <Connection Input="0" LocalName=".Enable" Model="CDPPortConnection" Name="MapEnable"></Connection>
        <Connection Input="0" LocalName=".ModeSelect" Model="CDPPortConnection" Name="MapModeSelect"></Connection>
        <Connection Input="0" LocalName=".SpeedRef" Model="CDPPortConnection" Name="MapSpeedRef"></Connection>
        <Connection Input="0" LocalName=".TorqueRef" Model="CDPPortConnection" Name="MapTorqueRef"></Connection>
        <Connection Input="0" LocalName=".TorqueLimitMotoring" Model="CDPPortConnection" Name="MapTorqueLimitMotoring"></Connection>
        <Connection Input="0" LocalName=".TorqueLimitGeneratoring" Model="CDPPortConnection" Name="MapTorqueLimitGeneratoring"></Connection>
        <Connection Input="0" LocalName=".PowerLimitMotoring" Model="CDPPortConnection" Name="MapPowerLimitMotoring"></Connection>
        <Connection Input="0" LocalName=".PowerLimitGeneratoring" Model="CDPPortConnection" Name="MapPowerLimitGeneratoring"></Connection>
        <Connection Input="0" LocalName=".LoadDrooping" Model="CDPPortConnection" Name="MapLoadDrooping"></Connection>
        <Connection Input="0" LocalName=".MasterFollowerMode" Model="CDPPortConnection" Name="MapMasterFollowerMode"></Connection>
        <Connection Input="1" LocalName=".StatusFlags" Model="CDPPortConnection" Name="MapStatusFlags"></Connection>
        <Connection Input="1" LocalName=".SpeedActual" Model="CDPPortConnection" Name="MapSpeedActual"></Connection>
        <Connection Input="1" LocalName=".TorqueActual" Model="CDPPortConnection" Name="MapTorqueActual"></Connection>
      </Connections></Port>
    <Port DataConsistency="1" Input="1" Model="AntiBacklashLib.EncoderPort" Name="ENC1" Routing="AntiBacklashApp.CANOpenPCIIOServer.KublerF5888">

      <Connections>
        <Connection Input="1" LocalName=".position" Model="CDPPortConnection" Name="Mapposition" RemoteName=".TPDO1.Position"></Connection>
        <Connection Input="1" LocalName=".speed" Model="CDPPortConnection" Name="Mapspeed" RemoteName=".TPDO3.Speed"></Connection>
      </Connections></Port>
    <Port Input="1" Model="AntiBacklashLib.SimCmdPort" Name="SimCmd" Routing="..SpeedCmdSim.SimCmd">

      <Connections>
        <Connection Input="1" LocalName=".SpeedCMD" Model="CDPPortConnection" Name="MapSpeedCMD"></Connection>
        <Connection Input="1" LocalName=".AntiBacklashEnabled" Model="CDPPortConnection" Name="MapAntiBacklashEnabled"></Connection>
        <Connection Input="1" LocalName=".EnableFCs" Model="CDPPortConnection" Name="MapEnableFCs"></Connection>
      </Connections></Port>
    <Port Input="1" Model="AntiBacklashLib.ShaftPort" Name="S1" Routing="AntiBacklashApp.CANOpenPCIIOServer.FC1">

      <Connections>
        <Connection Input="1" LocalName=".ShaftAngle" Model="CDPPortConnection" Name="MapShaftAngle"></Connection>
        <Connection Input="1" LocalName=".ShaftRounds" Model="CDPPortConnection" Name="MapShaftRounds"></Connection>
      </Connections></Port>
    <Port Input="1" Model="AntiBacklashLib.ShaftPort" Name="S2" Routing="AntiBacklashApp.CANOpenPCIIOServer.FC2">

      <Connections>
        <Connection Input="1" LocalName=".ShaftAngle" Model="CDPPortConnection" Name="MapShaftAngle"></Connection>
        <Connection Input="1" LocalName=".ShaftRounds" Model="CDPPortConnection" Name="MapShaftRounds"></Connection>
      </Connections></Port>
    <Port Input="1" Model="AntiBacklashLib.ShaftPort" Name="S3" Routing="AntiBacklashApp.CANOpenPCIIOServer.FC3">

      <Connections>
        <Connection Input="1" LocalName=".ShaftAngle" Model="CDPPortConnection" Name="MapShaftAngle"></Connection>
        <Connection Input="1" LocalName=".ShaftRounds" Model="CDPPortConnection" Name="MapShaftRounds"></Connection>
      </Connections></Port>
  </Ports>

  <Parameters>
    <Parameter Name="LoadTorqueLimit" Unit="Nm" Value="4" Routing="..FullTest.LoadTorqueLimit"></Parameter>
    <Parameter Name="MaxTorque" Unit="Nm" Value="5" Routing="..FullTest.MaxTorque"></Parameter>
    <Parameter Name="SlaveTorqueBase" Unit="Nm" PreviousValue="3" Value="3" Routing="..FullTest.SlaveTorqueBase"></Parameter>
    <Parameter Name="SlaveTorqueGain" Value="5" Routing="..FullTest.SlaveTorqueGain"></Parameter>
    <Parameter Name="Offset" Unit="%" Value="1" Routing="..FullTest.Offset"></Parameter>
    <Parameter Name="SlaveDroop" Unit="%" Value="0" Routing="..FullTest.SlaveDroop"></Parameter>
    <Parameter Name="MasterDroop" Unit="%" Routing="..FullTest.MasterDroop"></Parameter>
  </Parameters>

  <Subcomponents></Subcomponents>
  
</Component>